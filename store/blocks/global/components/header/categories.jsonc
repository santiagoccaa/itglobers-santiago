{
    "flex-layout.row#header__global__row-2": {
        "title": "Main Desktop Header Flex Row Categories",
        "props": {
            "horizontalAlign": "left",
            "verticalAlign": "center",
            "preventHorizontalStretch": true,
            "preventVerticalStretch": true,
            "fullWidth": true,
            "marginTop": "3",
            "preserveLayoutOnMobile": true,
            "blockClass": "row2-desktop"
